{
  "rewrites": [
    { "source": "/", "destination": "/public/landing.html" },
    { "source": "/checkout", "destination": "/public/checkout.html" },
    { "source": "/thanks", "destination": "/public/thanks.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|webp|svg)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
